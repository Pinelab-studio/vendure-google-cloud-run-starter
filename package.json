{
  "name": "vendure-google-cloud-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "node ./dist/index",
    "serve": "nodemon -e ts --watch './src/' --exec \"yarn build && node ./dist/index\"",
    "serve:prod": "export ENV_FILE=.env && yarn serve",
    "serve:test": "export ENV_FILE=.env.test && yarn serve",
    "build": "rimraf dist && yarn tsc",
    "build:admin": "rimraf src/__admin-ui && yarn ts-node src/compile-admin-ui.ts",
    "lint:check": "yarn prettier --check . && yarn eslint .",
    "lint:fix": "yarn prettier --write . && yarn eslint --fix .",
    "script:prod": "export ENV_FILE=.env && yarn ts-node",
    "script:test": "export ENV_FILE=.env.test && yarn ts-node",
    "generate-migration": "yarn ts-node migration generate",
    "generate-migration:test": "export ENV_FILE=.env.test && yarn generate-migration",
    "generate-migration:prod": "export ENV_FILE=.env && yarn generate-migration",
    "run-migration": "yarn ts-node migration run",
    "run-migration:test": "export ENV_FILE=.env.test && yarn run-migration",
    "run-migration:prod": "export ENV_FILE=.env && yarn run-migration",
    "revert-migration": "yarn ts-node migration revert",
    "revert-migration:test": "export ENV_FILE=.env.test && yarn revert-migration",
    "revert-migration:prod": "export ENV_FILE=.env && yarn revert-migration",
    "test": "jest --preset=\"ts-jest\""
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "@vendure/admin-ui-plugin": "1.9.3",
    "@vendure/asset-server-plugin": "1.9.3",
    "@vendure/core": "1.9.3",
    "@vendure/email-plugin": "1.9.3",
    "@vendure/ui-devkit": "^1.9.3",
    "dotenv": "16.0.3",
    "mysql": "2.18.1",
    "vendure-plugin-google-cloud-tasks": "^5.0.1",
    "vendure-plugin-google-storage-assets": "^5.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.16.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vendure/testing": "^1.9.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.3.5",
    "rimraf": "^4.4.0",
    "@types/jest": "^29.5.0"
  }
}
